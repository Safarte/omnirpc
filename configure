# Globus Toolkit check
GLOBUS_MAKE_DEF="config.globus"
GLOBUS_FLAVOR="gcc32dbgpthr"
GLOBUS_MAKE_FILE_HEADER=""
GLOBUS_INCLUDE=""
GLOBUS_LIBS=""
GLOBUS_LIBS_MODULE="globus_io globus_common globus_gram_client globus_data_conversion"
GLOBUS_CPPFLAGS=""
GLOBUS_PATH=""
GLOBUS_GPT_PATH=""

if test "${enable_globus+set}" = set; then
  echo "checking Globus Toolkit Enable"
  CPPFLAGS="${CPPFLAGS}"" -DUSE_GLOBUS"
  if test "${with_globusflavor+set}" = set ; then
    GLOBUS_FLAVOR="${with_globusflavor}"
    if echo ${GLOBUS_FLAVOR} |grep pthr\$ ; then
      true
    else
      { echo "configure: error: ${GLOBUS_FLAVOR} flavor dosen't support pthread." 1>&2; exit 1; }
    fi
  fi
  if test "${with_globusdir+set}" = set ; then
    GLOBUS_PATH="${with_globusdir}"
  else
    if test -z "${GLOBUS_LOCATION}" ; then
      { echo "configure: error: Globus Toolkit Location is not defined." 1>&2; exit 1; }
    else
	GLOBUS_PATH="${GLOBUS_LOCATION}"
    fi
  fi
  echo "checking Globus Toolkit Location... ${GLOBUS_PATH}"
  if test "${with_gptdir+set}" = set; then
    GLOBUS_GPT_PATH="${with_gptdir}"
  else
    if test -z "${GPT_LOCATION}" ; then
      { echo "configure: error: GPT Location is not defined." 1>&2; exit 1; }
    else
	GLOBUS_GPT_PATH="${GPT_LOCATION}"
    fi
  fi
  echo "checking GPT Location... ${GLOBUS_GPT_PATH}"
    GLOBUS_MAKE_FILE_HEADER="${GLOBUS_PATH}/bin/globus-makefile-header"
  echo "checking Globus Toolkit globus_makefile_header... ${GLOBUS_MAKE_FILE_HEADER}"
  echo "checking Globus Toolkit flavor... ${GLOBUS_FLAVOR}"
  if test ! -d "${GLOBUS_PATH}/include/${GLOBUS_FLAVOR}" ; then
    { echo "configure: error: cannot find ${GLOBUS_FLAVOR} flavor." 1>&2; exit 1; }
  fi
  echo "creating Globus Toolkit makefile header"
  if test -f "${GLOBUS_MAKE_DEF}" ; then
    rm "${GLOBUS_MAKE_DEF}"
  fi
  if `GLOBUS_LOCATION=${GLOBUS_PATH} GPT_LOCATION=${GLOBUS_GPT_PATH} ${GLOBUS_MAKE_FILE_HEADER} --flavor=${GLOBUS_FLAVOR} ${GLOBUS_LIBS_MODULE} > ${GLOBUS_MAKE_DEF}` ; then
    true
  else
    { echo "configure: error: cannot create makefile header for globus toolkit.(command failed)" 1>&2; exit 1; }
  fi
  if test ! -f "${GLOBUS_MAKE_DEF}" ; then
    { echo "configure: error: cannot create makefile header for globus toolkit." 1>&2; exit 1; }
  fi
  GLOBUS_INCLUDE=`grep GLOBUS_INCLUDE ${GLOBUS_MAKE_DEF}| sed "s/.*=.*-I//"`
  echo "checking Globus Toolkit include... ""${GLOBUS_INCLUDE}"
  GLOBUS_INCLUDE_PATH=`echo "${GLOBUS_INCLUDE}"|sed "s/.*-I//"`
  if test ! -d "${GLOBUS_INCLUDE}" ; then
    { echo "configure: error: cannot find ${FLAVOR} include file." 1>&2; exit 1; }
  else
    echo "checking Globus Toolkit library... ${GLOBUS_PATH}/lib"
    GLOBUS_LIBS_OPT=`grep GLOBUS_PKG_LIBS ${GLOBUS_MAKE_DEF} | sed "s/.*=//"`
    echo "checking Globus Toolkit linker Options... ${GLOBUS_LIBS_OPT}"
    GLOBUS_LIBS="-L${GLOBUS_PATH}/lib ${GLOBUS_LIBS_OPT}"
  fi
  #setting compiler option
  CPPFLAGS="${CPPFLAGS} -I${GLOBUS_INCLUDE}"
fi



